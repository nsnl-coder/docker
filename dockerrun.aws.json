{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "client",
      "image": "nsnhatlong/client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "api",
      "image": "nsnhatlong/api",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "nsnhatlong/worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "nsnhatlong/nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "host": 80,
          "containerPort": 80
        }
      ]
    }
  ]
}
